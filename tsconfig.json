{
    "compilerOptions": {
        "target": "es2016",
        "module": "es2015",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "downlevelIteration": true,
        "strictNullChecks": true,
        "allowJs": true
    },
    "include": [
        "src",
        "node_modules/imgui"
    ],
    "exclude": [
        "__tests__",
        "dist"
    ]
}
