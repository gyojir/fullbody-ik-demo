{
    "compilerOptions": {
        "target": "es5",
        "module": "es2015",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "downlevelIteration": true,
        "strictNullChecks": true
    },
    "include": [
        "."
    ],
    "exclude": [
        "node_modules"
    ]
}
